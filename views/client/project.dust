	<h2>{.customer}</h2>
	<div id="slider" class="swipe">
		<div class="swipe-wrap">
			{#.photoset}
				<div><img src="{.}" alt="" /></div>
			{/.photoset}
		</div>
	</div>
	<span>Link:
		{?.link}<a href="{.link}">{.link}</a>{/.link}
